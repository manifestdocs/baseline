@prelude(script)

fn divisor_sum(i: Int, j: Int, acc: Int) -> Int =
  if j > i then acc
  else if i % j == 0 then divisor_sum(i, j + 1, acc + j)
  else divisor_sum(i, j + 1, acc)

fn total_divsum(n: Int, i: Int, acc: Int) -> Int =
  if i > n then acc
  else total_divsum(n, i + 1, acc + divisor_sum(i, 1, 0))

fn main!() -> {Console} () = Console.println!("${total_divsum(10000, 1, 0)}")
