@prelude(core)

type Point = { x: Int, y: Int }

// Function Signature Mismatch (Return Type)
fn bad_ret(x: Int) -> Int = {
    "string" // Returns String, expects Int
}

// Function Arg Mismatch
fn bad_args(a: Int) -> Int = { // Expects 2 args, provides 1
    a
}

fn main() -> Int = {
  // Field Mismatch
  let p = Point { x: true, y: 1 }

  // Missing Field
  let p2 = Point { x: 1 }
  
  // Call Mismatch
  let res = bad_args(1, "2") // Arg 2 expects Int

  0
}
