@prelude(script)

// Tier 1: Fibonacci â€” recursion, conditionals, arithmetic

fn fibonacci(n: Int) -> Int = if n <= 1 then n else fibonacci(n - 1) + fibonacci(n - 2)

fn main!() -> {Console} () = {
  Console.println!("fib(0) = ${fibonacci(0)}")
  Console.println!("fib(1) = ${fibonacci(1)}")
  Console.println!("fib(5) = ${fibonacci(5)}")
  Console.println!("fib(10) = ${fibonacci(10)}")
  Console.println!("fib(15) = ${fibonacci(15)}")
}
