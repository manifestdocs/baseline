@prelude(script)

// Tier 1: Factorial â€” recursion, multiplication

factorial : Int -> Int
factorial = |n| if n <= 1 then 1 else n * factorial(n - 1)

main! : () -> {Console} ()
main! = || {
  Console.println!("0! = ${factorial(0)}")
  Console.println!("1! = ${factorial(1)}")
  Console.println!("5! = ${factorial(5)}")
  Console.println!("10! = ${factorial(10)}")
}
