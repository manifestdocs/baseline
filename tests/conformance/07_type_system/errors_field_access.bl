@prelude(core)

// Negative test: field access errors
// Expected errors: TYP_014, TYP_015

type Point = { x: Int, y: Int }

// TYP_014: field not found on struct
fn bad_field() -> Int = {
  let p = Point { x: 1, y: 2 }
  p.z
}

// TYP_015: field access on non-struct
fn bad_access() -> Int = {
  let x = 42
  x.field
}
